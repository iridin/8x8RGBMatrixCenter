<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MatrixView">
        <attr name="ledMargin" format="dimension" />
        <attr name="ledBackground" format="color" />
    </declare-styleable>

    <declare-styleable name="ColorSelectionView">
        <attr name="colorMargin" format="dimension" />
        <attr name="colorCount" format="integer" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
</resources>